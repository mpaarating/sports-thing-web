{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "app"
    ],
    "exclude": [
      "app/bower_components"
    ],
    "excludePattern": "\\.min\\.js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  }
}
